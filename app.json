{
  "expo": {
    "name": "RNBarChart",
    "description": "Bar Chart for react-native responsive, with many options, and without library. Author : @JulienKermarec",
    "slug": "react-native-bar-chart",
    "privacy": "public",
    "sdkVersion": "21.0.0",
    "version": "1.0.0",
    "orientation": "portrait",
    "primaryColor": "#cccccc",
    "icon": "./assets/icons/app-icon.png",
    "notification": {
      "icon": "./assets/icons/notification-icon.png",
      "color": "#000000"
    },
    "loading": {
      "icon": "./assets/icons/loading-icon.png",
      "hideExponentText": false
    },
    "packagerOpts": {
      "assetExts": ["ttf"]
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.julienkermarec.rnbarchart"
    },
    "android": {
      "package": "com.hotelskipper.rnbarchart"
    }
  }
}
